{
  "name": "alphaTanTen",
  "description": "A Chrome Extension to make reservation of the tennis court",
  "version": "2.2",
  "manifest_version": 2,
  "web_accessible_resources": ["alphatanten.js"],
  "content_scripts": [
    {
      "matches": [
        "http://sports.isdc.co.kr/member/login",
        "http://sports.isdc.co.kr/rent/reservation/index/*",
        "http://sports.isdc.co.kr/rent/inputform",
        "http://time.navyism.com/?host=sports.isdc.co.kr",
        "http://sports.isdc.co.kr/message"
      ],
      "js": ["contentscript.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "img/tennis16.png",
    "48": "img/tennis48.png",
    "128": "img/tennis128.png"
  }
}
